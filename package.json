{
  "name": "platform-notification",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/__tests__/**/*.ts\"",
    "start": "nest start",
    "start:local": "dotenv -e .env.local -- nest start --watch",
    "start:dev": "dotenv -e .env.dev nest start --watch",
    "start:test": "dotenv -e .env.test nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "prisma migrate deploy && NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint --fix \"{src,apps,libs,test}/**/*.ts\"",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "dotenv -e .env.local prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --force",
    "migrate:reset:test": "dotenv -e .env.test prisma migrate reset --force",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "migrate:test": "dotenv -e .env.test npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "pretest": "docker-compose -f docker-compose.test.yaml --env-file .env.test up -d && sleep 3 && npm run migrate:test",
    "test": "dotenv -e .env.test jest --continue-on-failure || :",
    "posttest": "docker-compose -f docker-compose.test.yaml down",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config src/__tests__/jest-e2e.json",
    "test:service": "jest --config src/__tests__/jest-service.json",
    "start:db": "npm run migrate:deploy && npm run prisma:generate && npm run seed",
    "postinstall": "npm run prisma:generate",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yaml up --build -d",
    "docker:db": "docker-compose -f docker-compose.db.yaml up -d",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@apollo/gateway": "^2.0.5",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@nestjs/apollo": "^10.0.22",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "9.0.9",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "9.0.9",
    "@nestjs/graphql": "^10.0.22",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^9.0.11",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.0.9",
    "@nestjs/swagger": "^5.2.0",
    "@prisma/client": "^4.3.1",
    "@types/passport-jwt": "^3.0.6",
    "amqp-connection-manager": "^4.1.6",
    "amqplib": "^0.10.2",
    "apollo-server-core": "^3.10.1",
    "apollo-server-express": "^3.10.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "email-templates": "^10.0.1",
    "express-ctx": "^0.1.1",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "nestjs-config": "^1.4.10",
    "nestjs-i18n": "^9.0.12",
    "nestjs-prisma": "^0.15.1",
    "nodemailer": "^6.7.8",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prisma-dbml-generator": "^0.8.3",
    "query-string": "^7.1.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "swagger-ui-express": "^4.3.0",
    "ts-morph": "^15.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.9",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "^7.0.0",
    "jest": "27.5.1",
    "prettier": "^2.6.2",
    "prisma": "4.2.1",
    "supertest": "6.2.2",
    "ts-jest": "27.1.3",
    "ts-loader": "9.2.8",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.0",
    "typescript": "4.6.2",
    "validate-branch-name": "^1.3.0"
  },
  "validate-branch-name": {
    "pattern": "^(master|main|dev|staging){1}$|^(feat|fix|hotfix|release|refactor)/.+$",
    "errorMsg": "Branch name must be one of the following: master, main, dev, staging, feat/<feature-name>, fix/<issue-number>, hotfix/<issue-number>, release/<version-number>, refactor/<issue-number>"
  }
}
